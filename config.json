{
  "webserver": {
    "port": 3000,
    "apiRoute": "/api"
  },

  "rsync": {

    "from": "./tests/_share/from",

    "target": "./tests/_share/target",

    "ignoreFileName": "ignore.lst",

    "defaultArgs": [
      "-arF",
      "--exclude='.rsync-filter'"
    ]
  },
  "log": {
    "appenders":[
      { "type": "file", "filename":"log.log" },
      { "type": "file", "filename":"requests.log", "category": [
        "webapp.request", "socket.request", "rsync.request"] }
    ]
  }
}